        "@types/js-cookie": "^2.2.6",
        "@types/node": "^16.10.2",
        "argon2-browser": "^1.18.0",
        "base64-loader": "^1.0.0",
        "bootstrap": "4",
        "buffer": "^6.0.3",
        "classnames": "^2.3.1",
        "clean-css": "^5.0.1",
        "clean-css-cli": "^5.1.0",
        "clipboard": "2.0.8",
        "csslint": "^1.0.5",
        "csstype": "^3.0.6",
        "jquery": "3.6.0",
        "js-cookie": "3.0.1",
        "lodash": "^4.17.20",
        "moment": "^2.29.4",
        "react": "^17.0.1",
        "react-bootstrap": "^2.7.2",
        "react-dom": "^17.0.1",
        "react-tooltip": "^4.2.14",
        "tweetnacl": "^1.0.3",
        "typescript": "^4.4.4",
        "uglify-js": "^3.12.8"
    },
    "devDependencies": {
        "@types/clipboard": "^2.0.1",
