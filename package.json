        "@typescript-eslint/parser": "^5.58.0",
        "css-loader": "^6.7.3",
        "css-modules-typescript-loader": "^4.0.1",
        "eslint": "^6.1.3",
        "eslint-config-google": "^0.14.0",
        "eslint-plugin-react": "^7.32.2",
        "react-scripts": "^5.0.1",
        "source-map-loader": "^3.0.0",
        "style-loader": "^3.3.2",
        "tablesorter": "^2.31.3",
        "ts-loader": "^9.2.6",
        "webpack": "5",
        "webpack-bundle-analyzer": "^4.4.0",
        "webpack-cli": "^4.9.1"
    }
